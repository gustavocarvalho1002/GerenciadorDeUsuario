<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cebraspe</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color: #00376f">
        <div class="container">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-6">
                        <h3 style="color: white">REGISTRO ACADÊMICO <i>ONLINE</i> - Cadastro colaborador</h3>
                    </div>
                    <div class="col-md-6">
                        <img class="logo-cebraspe.png pull-right" src="http://www.cespe.unb.br/cdn/images/logo-cebraspe.png" alt="logo-cebraspe" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />



    </div>

    <div class="navbar navbar-inverse navbar-fixed-bottom" style="background-color: #00376f">
        <div class="container">
            <div class="text-center" style="color: white">
                &copy; @DateTime.Now.Year - CEBRASPE - CPDR
            </div>
        </div>
    </div>


    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>

</html>